<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, 
Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">

<web-app>

	<env-entry>
	<env-entry-name>rmIp_flight</env-entry-name>
	<env-entry-value>localhost</env-entry-value>
	<env-entry-type>java.lang.String</env-entry-type>
</env-entry>
<env-entry>
	<env-entry-name>rmPort_flight</env-entry-name>
	<env-entry-value>38081</env-entry-value>
	<env-entry-type>java.lang.Integer</env-entry-type>
</env-entry>

<env-entry>
	<env-entry-name>rmIp_car</env-entry-name>
	<env-entry-value>localhost</env-entry-value>
	<env-entry-type>java.lang.String</env-entry-type>
</env-entry>
<env-entry>
	<env-entry-name>rmPort_car</env-entry-name>
	<env-entry-value>38082</env-entry-value>
	<env-entry-type>java.lang.Integer</env-entry-type>
</env-entry>

<env-entry>
	<env-entry-name>rmIp_room</env-entry-name>
	<env-entry-value>localhost</env-entry-value>
	<env-entry-type>java.lang.String</env-entry-type>
</env-entry>
<env-entry>
	<env-entry-name>rmPort_room</env-entry-name>
	<env-entry-value>38083</env-entry-value>
	<env-entry-type>java.lang.Integer</env-entry-type>
</env-entry>

<env-entry>
	<env-entry-name>rmIp_customer</env-entry-name>
	<env-entry-value>localhost</env-entry-value>
	<env-entry-type>java.lang.String</env-entry-type>
</env-entry>
<env-entry>
	<env-entry-name>rmPort_customer</env-entry-name>
	<env-entry-value>38084</env-entry-value>
	<env-entry-type>java.lang.Integer</env-entry-type>
</env-entry>

    <listener>
        <listener-class>
            com.sun.xml.ws.transport.http.servlet.WSServletContextListener
        </listener-class>
    </listener>

    <servlet>
        <servlet-name>service</servlet-name>
        <servlet-class>
        	com.sun.xml.ws.transport.http.servlet.WSServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>service</servlet-name>
        <url-pattern>/service</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>120</session-timeout>
    </session-config>

</web-app>
